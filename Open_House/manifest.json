{
  "manifest_version": 2,
  "name": "Open_house",
  "description": "Customizable Homepage by Moises Beleche",
  "version": "1.0",
  "chrome_url_overrides": {
      "newtab": "newpage.html"
  },
  "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "browser_action": {
      "default_icon": "images/icon48.png"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "web_accessible_resources": [
      "newpage.html",
      "newpage.js",
      "images/icon16.png",
      "images/icon48.png",
      "images/icon128.png"
  ]
}
